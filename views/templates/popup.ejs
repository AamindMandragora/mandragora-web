<div id="<%= title.toLowerCase() %>-shade" class="shade"></div>
<div id="<%= title.toLowerCase() %>-popup" class="popup">
    <button class="exit">X</button>
    <h1><%- title %></h1>
    <div id="<%= title.toLowerCase() %>-data" class="data">
        <% for (var i = 0; i < elements.length; i++) { %>
            <%= elements[i][0] %>~<%= elements[i][1] %>~<%= elements[i][2] %>||
        <% } %>
    </div>
</div>